    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.2",
    "react-masonry-css": "^1.0.16",
    "react-photo-album": "^2.3.0",
    "react-scripts": "5.0.1",
    "uuid": "^11.1.0",
